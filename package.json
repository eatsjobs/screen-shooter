{
  "name": "odigeo-error-reporting",
  "version": "1.0.3",
  "description": "Error reporting module",
  "source": "src/main.js",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "unpkg": "lib/index.umd.js",
  "browser": "lib/index.umd.js",
  "publishConfig": {
    "registry": "http://nexus.services.odigeo.com/nexus/repository/npm-all/"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production microbundle src/main.js -f es,umd --sourcemap --compress=true --external=none",
    "build:dev": "microbundle src/main.js -f es,umd --sourcemap --compress=false --external=none",
    "build:watch": "microbundle src/main.js -f es,umd -w --sourcemap --compress=false --external=none",
    "serve": "http-server . -o -p 6009 -c-1",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel build:watch serve",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --watch --no-cache",
    "version": "npm run build && git add -A lib",
    "postversion": "git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/pmangialavori/odigeo-error-reporting.git"
  },
  "keywords": [],
  "author": "eatsjobs",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pmangialavori/odigeo-error-reporting#readme",
  "devDependencies": {
    "cross-env": "^6.0.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "esm": "=3.1.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "microbundle": "v0.12.0-next.6",
    "npm-run-all": "^4.1.5"
  },
  "files": [
    "lib/**"
  ],
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "htm": "^2.2.1",
    "html2canvas": "^1.0.0-rc.5",
    "preact": "^10.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
