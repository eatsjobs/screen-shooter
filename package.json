{
  "name": "odigeo-error-reporting",
  "version": "1.0.0",
  "description": "Error reporting module",
  "browser": "dist/umd.js",
  "module": "dist/esm.mjs",
  "publishConfig": {
    "registry": "http://nexus.services.odigeo.com/nexus/repository/npm-all/"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:watch": "rollup -cw",
    "serve": "http-server . -o -p 6009",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel build:watch serve",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/pmangialavori/odigeo-error-reporting.git"
  },
  "keywords": [],
  "author": "eatsjobs",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pmangialavori/odigeo-error-reporting#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-jest": "^24.9.0",
    "camelcase": "^5.3.1",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "np": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "files": [
    "dist/**"
  ],
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "browserslist": {
    "production": [
      "last 2 version",
      "> 1%",
      "not dead",
      "IE 11",
      "not op_mini all",
      "node 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {}
}
