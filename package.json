{
  "name": "odigeo-error-reporting",
  "version": "1.0.0",
  "description": "Error reporting module",
  "source": "src/main.js",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "unpkg": "lib/index.umd.js",
  "publishConfig": {
    "registry": "http://nexus.services.odigeo.com/nexus/repository/npm-all/"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "microbundle src/main.js -f es,umd --sourcemap --compress",
    "build:dev": "microbundle src/main.js -f es,umd --sourcemap --compress=false",
    "build:watch": "microbundle src/main.js -f es,umd -w --sourcemap",
    "serve": "http-server . -o -p 6009",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel build:watch serve",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --watch --no-cache",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/pmangialavori/odigeo-error-reporting.git"
  },
  "keywords": [],
  "author": "eatsjobs",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pmangialavori/odigeo-error-reporting#readme",
  "devDependencies": {    
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "esm": "=3.1.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "microbundle": "v0.12.0-next.6"
  },
  "files": [
    "lib/**"
  ],
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "browserslist": {
    "production": [
      "last 2 version",
      "> 1%",
      "not dead",
      "IE 11",
      "not op_mini all",
      "node 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
